class Solution {
    public int reverse(int x) {
        int reversen = 0;
           int number = x;
           
            while(number != 0)   
            {  
                int remainder = number % 10;  
                reversen = reversen * 10 + remainder;  
                number = number/10;  
            }  
            return reversen;
            
        }
    }
